{
    "name": "project_name",
    "version": "0.0.1",
    "description": "[PROJECT_NAME]",
    "main": "index.js",
    "scripts": {
        "start": "npm run tasks && node server/index.js",
        "local": "./run.local.sh",
        "tasks": "node tasks",
        "doc": "vuese gen --include='./src/components/action/*.vue' --outDir='./docs/'",
        "build": "rimraf public && npm run tasks && npm run build:client && npm run build:server ",
        "build:client": "webpack --config build/webpack.client.config.js --progress",
        "build:server": "webpack --config build/webpack.server.config.js --progress",
        "build:prod": "rimraf public && npm run tasks && npm run build:clientprod && npm run build:serverprod ",
        "build:clientprod": "webpack --config build/webpack.client.config.js --progress --env=production ",
        "build:serverprod": "webpack --config build/webpack.server.config.js --progress --env=production",
        "pretty": "./node_modules/.bin/prettier --config ./.prettierrc --write",
        "precommit": "lint-staged",
        "test": "jest --updateSnapshot --projects  src server --no-cache --detectOpenHandles --json --outputFile=./coverage/coverage.json"
    },
    "jest": {
        "projects": [
            "src/jest.config.js",
            "server/jest.config.js"
        ]
    },
    "lint-staged": {
        "*.{js,vue}": [
            "npm run pretty",
            "git add"
        ]
    },
