{
  "name": "project_name",
  "version": "0.0.1",
  "description": "[PROJECT_NAME]",
  "main": "index.js",
  "scripts": {
    "start": "npm run tasks && node server/index.js",
    "tasks": "node tasks",
    "doc": "vuese gen --include='./src/components/action/*.vue' --outDir='./docs/'",
    "build": "rimraf public && npm run tasks && npm run build:client && npm run build:server ",
    "build:client": "webpack --config build/webpack.client.config.js --progress --hide-modules ",
    "build:server": "webpack --config build/webpack.server.config.js --progress --hide-modules",
    "build:prod": "rimraf public && npm run tasks && npm run build:clientprod && npm run build:serverprod ",
    "build:clientprod": "webpack --config build/webpack.client.config.js --progress --hide-modules --env=production ",
    "build:serverprod": "webpack --config build/webpack.server.config.js --progress --hide-modules --env=production",
    "pretty": "./node_modules/.bin/prettier --config ./.prettierrc --write './src/**/*.*'",
    "precommit": "lint-staged",
    "test": "jest  --projects  src server --no-cache"
  },
  "jest": {
    "projects": [
      "src/jest.config.js",
      "server/jest.config.js"
    ]
  },
  "lint-staged": {
    "*.{js,vue}": [
      "npm run pretty",
      "git add"
    ]
  },
